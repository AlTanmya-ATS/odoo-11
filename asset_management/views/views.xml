               <!--book-assets form view -->
        <record model="ir.ui.view" id="book_assets_form_view">
            <field name="name">book_assets.form</field>
            <field name="model">asset_management.book_assets</field>
            <field name="arch" type="xml">
                <form string="Book Asset Form" create="false" >
                    <header>
                        <button name="%(asset_dep_modify_action)d" string="Modify Depreciation" type="action" class="oe_highlight" attrs="{'invisible':['|',('depreciated_flag','=',False),('state','!=','open')]}"/>
                        <button name="validate"  type="object" string="confirm" class="oe_highlight" attrs="{'invisible':['|',('asset_state','!=','capitalize'),('state','!=','draft')]}"/>
                        <button name="set_to_draft" string="Set to Draft" type="object" attrs="{'invisible': ['|',('state','!=','open'),('entry_count','!=',0)]}"/>
                        <button name="compute_depreciation_board" string="Compute Depreciation" type="object" class="oe_highlight"
                                attrs="{'invisible':['|','|',('depreciated_flag','=',False),('asset_state','!=','capitalize'),('state','!=','draft')]}"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,open"/>
                    </header>
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <button class="oe_stat_button" name="open_entries"  type="object" icon="fa-pencil">
                                <field string="Items" name="entry_count" widget="statinfo" />
                             </button>
                        </div>
					    <group>
                            <group>
						        <field name="name" invisible="1"/>
                                <field name="book_id"/>
							    <field name="asset_id"/>
                                <field name="original_cost"/>
                                <field name="salvage_value_type"/>
                                <label for="salvage_value_amount"/>
                                <dive>
                                    <field name="salvage_value_amount" class="oe_inline"/>
                                    <label string="%" class="oe_inline" attrs="{'invisible':[('salvage_value_type','=','amount')]}"/>
                                </dive>
                                <field name="salvage_value"/>
							    <field name="current_cost"/>
                                <field name="asset_state" invisible="1"/>

                            </group>
						    <group>
						        <field name="depreciated_flag" attrs="{'readonly':[('depreciation_line_length','!=',0)]}"/>
                                <field name="depreciation_line_length" invisible="1"/>
						        <field name="method"/>
                                <field name="method_progress_factor" attrs="{'invisible':[('method','=','linear')]}"/>
                                <field name="method_time"/>
							    <field name="date_in_service"/>
							    <field name="prorate_date" readonly="1"/>
							    <!--<field name="prorate_convenction"/>-->
                                <field name="method_number" attrs="{'invisible':[('method_time','=','end')],'required':[('method_time','=','number')]}"/>
							    <field name="life_months"/>
                                <field name="end_date" attrs="{'invisible':[('method_time','=','number')]}"/>
						    </group>
					    </group>
                        <notebook>
                        <page name="depreciation" string="Depreciation">
                                <field name="depreciation_line_ids" widget="One2many_list">
                                    <tree string="Depreciation" edit="false" create="false">
                                        <field name="depreciation_date"/>
                                        <field name="depreciated_value"/>
                                        <field name="amount"/>
                                        <field name="remaining_value"/>
                                        <field name="parent_state" invisible="1"/>
                                        <field name="move_check" widget="deprec_lines_toggler" attrs="{'invisible':[('parent_state','!=','open')]}"/>
                                        <field name="move_posted_check" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>     
            </field>
        </record>

