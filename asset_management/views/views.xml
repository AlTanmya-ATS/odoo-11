<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data>

               <!--Retirement  form view -->
        <record model="ir.ui.view" id="retirement_form_view">
            <field name="name">retirement.form</field>
            <field name="model">asset_management.retirement</field>
            <field name="arch" type="xml">
                <form string="Retirement Form">
                    <sheet>
					  <group>
                        <group>
						    <field name="name"/>
                            <field name="book_id"/>
                            <field name="asset_id"/>
                            <field name="retire_date"/>
							<field name="residual_value"/>
							<field name="units_retired"/>
							<field name="current_units"/>
                        </group>
						<group>
						    <field name="gain_loss_amount"/>
						    <field name="proceeds_of_sale"/>
							<field name="partner_id"/>
							<field name="cost_of_removal"/>
							<field name="check_invoice"/>
						</group>
					  </group>
                      <notebook>
                          <page string="Comments">
                               <field name="comments"/>
                            </page>
                      </notebook>
                    </sheet>
                </form>
            </field>
        </record>
		            <!--retirement list view -->
	    <record model="ir.ui.view" id="retirement_tree_view">
	         <field name="name">retirement.tree</field>
			 <field name="model">asset_management.retirement</field>
			 <field name="arch" type="xml">
			     <tree string="Retirement Tree">
                     <field name="name"/>
				     <field name="retire_date"/>
				 </tree>
			 </field>
	    </record>

		<!--Retirement search view -->
		<record model="ir.ui.view" id="retirement">
		     <field name="name">retirement.search</field>
			 <field name="model">asset_management.retirement</field>
			 <field name="arch" type="xml">
			     <search>
                     <field name="book_assets_id"/>
				     <field name="retire_date"/>
				 </search>
		     </field>
		</record>

		         <!--retirement action window-->
        <record model="ir.actions.act_window" id="retirement_list_action">
            <field name="name">Retirement</field>
            <field name="res_model">asset_management.retirement</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context" eval="{'search_default_my_retirement': 1}"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Create the first retirement
                </p>
            </field>
        </record>


                           <!--open retirement ui by button -->
         <record model="ir.actions.act_window" id="set_asset_to_retire">
             <field name="name">set asset to retire</field>
             <field name="res_model">asset_management.retirement</field>
             <field name="view_mode">form</field>
             <field name="view_id" ref="retirement_form_view"/>
             <field name="target">current</field>
             <field name="context">{'default_asset_id':active_id}</field>
         </record>


               <!--asset form view -->
        <record model="ir.ui.view" id="asset_form_view">
            <field name="name">asset.form</field>
            <field name="model">asset_management.asset</field>
            <field name="arch" type="xml">
                <form string="Asset Form">
                    <header>
                        <field name="state" widget="statusbar" statusbar_visible="draft,capitalize"/>
                        <button string="Set to Retire" type="action" name="%(set_asset_to_retire)d" class="oe_highlight"/>
                        <button string="Retirement" type="object" name="open_retired_window" class="oe_highlight"/>
                    </header>
                    <sheet>
                        <div name="oe_left oe_title">
                            <label for="name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
					    <group>
                            <group>
                                <field name="item_id"/>
                                <field name="category_id"/>
                                <field name="parent_asset"/>
                                <field name="default_book"/>
                                <field name="gross_value" placeholder="Asset Gross Value in default book" attrs="{'readonly':[('id','!=',False)]}"/>
                                <field name="asset_with_category" invisible="1"/>
                            </group>
                            <group>
                             <!--   <field name="units"/> -->
                                <field name="in_use_flag"/>
                                <field name="is_in_physical_inventory"/>
                                <field name="is_new"/>
                                <field name="ownership_type"/>
                                <field name="asset_serial_number"/>
                                <field name="asset_tag_number" widget="many2many_tags" options="{'color_field': 'color'}"/>
                                <!--'no_create_edit': True/>-->
                            </group>
					    </group>
                        <notebook>
                            <page string="Description">
                                   <field name="description"/>
                            </page>

                            <page name="book" string="Book">
                                <field name="book_assets_id"  widget="One2many_list" context="{'category_id':category_id,'default_book':default_book}">
                                    <tree string="Book Num" editable="bottom">
                                        <field name="book_id"/>
                                        <field name="current_cost"/>
                                        <field name="original_cost"/>
                                        <field name="method"/>
                                        <field name="salvage_value_type"/>
                                        <field name="salvage_value_amount"/>
                                        <field name="date_in_service"/>
                                        <field name="asset_id" invisible="1"/>
                                        <button name="move_to_book_asset" type="object" string="Details" class="oe_highlight"/>
                                    </tree>
                                </field>
                            </page>

                            <page name="assignment" string="Assignment">
                                <field name="assignment_id" widget="One2many_list" context="{'category_id':category_id}">
                                    <tree string="Assignment">
                                        <field name="book_id"/>
                                        <field name="responsible_id"/>
                                        <field name="location_id"/>
                                        <field name="transfer_date"/>
                                        <field name = "percentage"/>
                                    </tree>
                                </field>
                            </page>

                            <page name=" SourceLine" string="Source Line">
                                <field name="source_line_id" widget="One2many_list">
                                        <tree string="Source Line">
                                                <field name="source_type"/>
                                                <field name="invoice_id"/>
                                                <field name="invoice_line_ids"/>
                                                <field name="amount"/>
                                        </tree>
                                </field>
                            </page>

                            <page name="depreciation" string="Depreciation">
                                <field name="depreciation_line_ids" widget="One2many_list" readonly="1">
                                    <tree string="Depreciation" create="false" edit="false" editable="false">
                                        <field name="book_id"/>
                                        <field name="depreciation_date"/>
                                        <field name="depreciated_value"/>
                                        <field name="amount"/>
                                        <field name="remaining_value"/>
                                        <field name="parent_state" invisible="1"/>
                                        <field name="move_check" widget="deprec_lines_toggler" attrs="{'invisible':[('parent_state','!=','open')]}"/>
                                        <field name="move_posted_check" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>	
		
		<!--asset list view -->
	    <record model="ir.ui.view" id="asset_tree_view">
	         <field name="name">asset.tree</field>
			 <field name="model">asset_management.asset</field>
			 <field name="arch" type="xml">
			     <tree string="Asset Tree">
