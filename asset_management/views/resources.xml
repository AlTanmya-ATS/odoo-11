<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
	<!-- Adds all assets in Odoo -->
        <template id="google_maps_backend" name="maps assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
		         <!-- All links to CSS and JS files should be added in here. -->
                 <script type="text/javascript" src="/asset_management/static/src/js/mapsapi.js"></script>
                <script src="/asset_management/static/src/js/callapi.js"
                   type="text/javascript">
                </script>
                <link rel="stylesheet" href="/asset_management/static/src/css/mapsstyle.css"/>
            </xpath>
        </template>
    </data>
</openerp>